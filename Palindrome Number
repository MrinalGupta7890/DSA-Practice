Leetcode-Q9-Palindrome-Number-Solution-Java

class Solution {
    public boolean isPalindrome(int x) {
        int rev = 0, z;
        z = x;
        while(x > 0){
            rev = (rev * 10) + x % 10;
            x = x / 10;
        }
        if(rev == z){
            return true;
        }else{
            return false;
        }
    }
}
